{
  "name": "cmd-blog",
  "version": "1.0.0",
  "private": true,
  "description": "A blog about the CMD course",
  "author": "M. Tervoort",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "dev": "vitepress dev src",
    "build": "vitepress build src",
    "preview": "vitepress preview src",
    "lint:js": "eslint . --ext \".vue,.js,.ts,.mts\" --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lint:fix": "yarn lint:prettier --write && yarn lint:js --fix",
    "test": "echo OK",
    "postinstall": "node scripts/warn-npm-install.js"
  },
  "lint-staged": {
    "*.{js,ts,mts,vue}": "eslint",
    "*": "prettier --ignore-unknown --check"
  },
  "engines": {
    "node": "^20.10.0",
    "yarn": "^4.0.2"
  },
  "packageManager": "yarn@4.1.0",
  "dependencies": {
    "@lando/vitepress-theme-default-plus": "latest",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "husky": "^9.0.10",
    "vitepress": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
